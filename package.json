{
  "name": "tsastgen",
  "version": "1.6.3",
  "description": "Generate TypeScript AST definitios from a specification file",
  "main": "lib/index.js",
  "bin": {
    "tsastgen": "lib/cli.js"
  },
  "scripts": {
    "prepare": "tsc",
    "test": "tsc -p test/ && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samvv/tsastgen.git"
  },
  "keywords": [
    "typescript",
    "abstract-syntax-tree",
    "specification-file",
    "declarative",
    "code-generator"
  ],
  "author": "Sam Vervaeck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samvv/tsastgen/issues"
  },
  "homepage": "https://github.com/samvv/tsastgen#readme",
  "dependencies": {
    "minimist": "^1.2.7",
    "source-map-support": "^0.5.21",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.19",
    "ava": "^5.1.1"
  },
  "ava": {
    "files": [
      "test/lib/*.test.js"
    ]
  }
